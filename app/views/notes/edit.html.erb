<%= form_with(model: @note, local: true) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title, placeholder: @note.title  %>
  </div>

  <div>
    <%= form.label :body %>
    <%= form.text_area :body, placeholder: @note.body %>
  </div>

    <%= form_with model: @post do |form| %>
    <%= form.label :content %>
    <%= marksmith_tag form, :content %>
    <%= form.submit %>



  <div>
    <%= form.submit "Update Note" %>
  </div>

